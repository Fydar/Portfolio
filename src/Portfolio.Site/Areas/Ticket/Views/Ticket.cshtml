@using Portfolio.Site.Areas.Ticket.Models;

@model TicketTicketViewModel
@{
  ViewData["Title"] = "Contact";
}

<section style="min-height: 80vh;">
  <div class="container">
    <h2>Ticket</h2>
    <div class="container">
      <p>
        @Model.TicketId
      </p>

      <p>
        @Model.From
      </p>

      <p>
        @Model.To
      </p>

      <div>
        @Html.Raw(Model.HtmlBody)
      </div>
    </div>
  </div>
</section>

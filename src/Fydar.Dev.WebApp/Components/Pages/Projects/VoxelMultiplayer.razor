@layout DefaultLayout
@page "/portfolio/voxelmultiplayer"

<HeadContent>
    @{
        var openGraphModel = new OpenGraphModel()
        {
            Title = "Voxel Multiplayer - Fydar",
            Description = "Fully bespoke voxel rendering system to create a 'Cube World' style game with a multiplayer component.",
            CanonicalUrl = "https://fydar.dev/portfolio/voxelmultiplayer",
            Properties = [
                new OpenGraphModelImage()
                {
                    Url = "https://fydar.dev/media/voxelmultiplayer/keyart-1_91x1x1200x630.webp",
                    Alt = "Promotional material for the game 'Voxel Multiplayer'."
                }
            ]
        };
    }
    <OpenGraphComponent Model="@openGraphModel" />
</HeadContent>

<section class="parallax-frame parallax-focalanchortop" style="--animation-time: 0.5;">

    <video class="parallax-layer" style="--parallax-multiplier: 0.5" autoplay muted loop>
        <source src="/media/voxelmultiplayer/background.mp4?v=1" type="video/mp4">
    </video>

</section>

<section class="landing-footer">

    <picture>
        <img class="titlecard" src="media/voxelmultiplayer/titlecard.svg?v=1" alt="Title card for the game 'The New Earth Conflict'." />
    </picture>

</section>

<section class="container">

    <h1>Fydar.Numerics</h1>



</section>
